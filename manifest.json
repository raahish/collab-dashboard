{
  "manifest_version": 2,
  "name": "Collab Dashboard",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "https://collab.its.virginia.edu/*"
      ],
      "js": ["content.js"]
    }
  ],
  "browser_action": {
  "default_icon": "icon.png"
  },
  "permissions": [
    "tabs"
  ],
  "background": {
    "matches": [
    "https://collab.its.virginia.edu/*"
    ],
    "scripts": [
      "background.js"
    ]
  }
}
